{
    "$schema": "https://biomejs.dev/schemas/1.1.2/schema.json",
    "organizeImports": {
        "enabled": true
    },
    "linter": {
        "enabled": true,
        "rules": {
            "recommended": true,
            "complexity": {
                "noForEach": "off"
            },
            "suspicious": {
                "noExplicitAny":"warn"
            }
        },
        "ignore": [
            "dist/*"
        ]
    }
}